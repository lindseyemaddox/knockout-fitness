  bottom: 20px

div.healcode,
.healcode
  primary-font($normal)
  padding-bottom: 70px

  div.list_view li div.signup
    top: 20px

  &.appointments
    max-width: 300px
    width: 100%

    .ui-datepicker
      max-width: 300px
      width: 100%

    a.hc-button
      button(100%)
      max-width: 300px

    .header
      margin-top: 6vh
      overflow-x: hidden
      height: 78vh
      margin-bottom: 3vh

      h1 .healcode-title-text,
      h1
        color: $white
        font-size: 23px
        letter-spacing: 2px
        primary-font($semibold)
        line-height: 1.6em
        text-transform: uppercase

      h1
        font-size: 2.4em
        line-height: 1em

        &.healcode-date-label
          font-size: 1.5em

      .trainer-label a
        color: $white
        font-size: .8em
        letter-spacing: .25em
        primary-font($bold)
        line-height: 3em

        &:hover
          color: $green
          transition: color 400ms ease

    .header .healcode-appointments p
      font-size: 1em
      letter-spacing: .1em
      primary-font($light)
      line-height: 2em

    .pre-filters
      margin: 30px 0

      select
        color: $grey
        letter-spacing: 0
        margin-bottom: 10px
        padding: 3px 0 5px
        primary-font($semibold)
        width: 100%
        max-width: 300px

      &:nth-child(2)
        color: $white
        font-size: 1em
        letter-spacing: 2px
        line-height: 2em
        primary-font($bold)

    a.action_icon
      display: none
      margin: 0
      color: $green
      font-size: 20px
      line-height: 1em
      padding: 0
      position: absolute
      right: 0
      top: -10px
      &.my_account_version
        margin: 0

      .action_text.my_account_text
        color: $green
        font-size: 12px
        primary-font($normal)
        line-height: 0
        letter-spacing: 0
        margin-right: 27px
        top: 1px
        position: relative

        &:hover
          color: $white
          transition: color 400ms ease

      img
        margin: 0

    .ui-state-default,
    .ui-datepicker-header a
      font-size: 1em

    &.list_view .header
      td,
      td.ui-state-disabled,
      td.ui-datepicker-current-day,
      td.ui-datepicker-week-end,
      td.ui-widget-content .ui-state-disabled,
      td.ui-widget-header .ui-state-disabled
        padding-top: 0!important
        vertical-align: top

    .ui-state-default,
    .ui-widget-content .ui-state-default,
    .ui-widget-header .ui-state-default,
    .ui-state-highlight,
    .ui-state-active
      padding: 10px 0
      text-align: center

  .bw-empty__clear
    display: none

  time
    display: inline-block

  .bw-widget
    width: 100%
    max-width: 300px
    margin: 0 auto

  .bw-calendar__heading
    font-weight: 100

  .bw-calendar__day
    font-weight: 700

#download
  background-color: #fff
  max-width: 330px
  padding: 60px 20px 20px
  width: 100%

  .store
    display: block

    img
      display: block
      width: 100%

    &:hover img
      opacity: .7
      transition: opacity 400ms ease

    &:first-child
      margin-bottom: 20px

.copyline
  position: absolute
  bottom: 50px
  right: 0

  span
    color: rgba($white, 0.5)
    font-size: 9px
    primary-font($semibold)
    text-align: center
    letter-spacing: .25em
    text-transform: uppercase

